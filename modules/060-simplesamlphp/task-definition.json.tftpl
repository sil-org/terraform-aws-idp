[
  {
    "volumesFrom": [],
    "memory": ${memory},
    "extraHosts": null,
    "dnsServers": null,
    "disableNetworking": null,
    "dnsSearchDomains": null,
    "portMappings": [
      {
        "containerPort": 80,
        "protocol": "tcp"
      },
      {
        "containerPort": 443,
        "protocol": "tcp"
      }
    ],
    "hostname": null,
    "essential": true,
    "entryPoint": null,
    "mountPoints": [],
    "name": "web",
    "ulimits": null,
    "dockerSecurityOptions": null,
    "environment": [
      {
        "name": "AWS_REGION",
        "value": "${aws_region}"
      },
      {
        "name": "ADMIN_EMAIL",
        "value": "${admin_email}"
      },
      {
        "name": "ADMIN_NAME",
        "value": "${admin_name}"
      },
      {
        "name": "ADMIN_PASS",
        "value": "${admin_pass}"
      },
      {
        "name": "BASE_URL_PATH",
        "value": "${base_url}"
      },
      {
        "name": "ENABLE_DEBUG",
        "value": "${enable_debug}"
      },
      {
        "name": "SECRET_SALT",
        "value": "${secret_salt}"
      },
      {
        "name": "SESSION_STORE_TYPE",
        "value": "sql"
      },
      {
        "name": "SHOW_SAML_ERRORS",
        "value": "${show_saml_errors}"
      },
      {
        "name": "THEME_COLOR_SCHEME",
        "value": "${theme_color_scheme}"
      },
      {
        "name": "PASSWORD_CHANGE_URL",
        "value": "${password_change_url}"
      },
      {
        "name": "PASSWORD_FORGOT_URL",
        "value": "${password_forgot_url}"
      },
      {
        "name": "HUB_MODE",
        "value": "${hub_mode}"
      },
      {
        "name": "ID_BROKER_ACCESS_TOKEN",
        "value": "${id_broker_access_token}"
      },
      {
        "name": "ID_BROKER_ASSERT_VALID_IP",
        "value": "${id_broker_assert_valid_ip}"
      },
      {
        "name": "ID_BROKER_BASE_URI",
        "value": "${id_broker_base_uri}"
      },
      {
        "name": "ID_BROKER_TRUSTED_IP_RANGES",
        "value": "${id_broker_trusted_ip_ranges}"
      },
      {
        "name": "LOGGING_LEVEL",
        "value": "${logging_level}"
      },
      {
        "name": "MFA_LEARN_MORE_URL",
        "value": "${mfa_learn_more_url}"
      },
      {
        "name": "MFA_SETUP_URL",
        "value": "${mfa_setup_url}"
      },
      {
        "name": "MYSQL_HOST",
        "value": "${mysql_host}"
      },
      {
        "name": "MYSQL_DATABASE",
        "value": "${mysql_database}"
      },
      {
        "name": "MYSQL_USER",
        "value": "${mysql_user}"
      },
      {
        "name": "MYSQL_PASSWORD",
        "value": "${mysql_password}"
      },
      {
        "name": "PARAMETER_STORE_PATH",
        "value": "${parameter_store_path}"
      },
      {
        "name": "PROFILE_URL",
        "value":  "${profile_url}"
      },
      {
        "name": "RECAPTCHA_SITE_KEY",
        "value": "${recaptcha_key}"
      },
      {
        "name": "RECAPTCHA_SECRET",
        "value": "${recaptcha_secret}"
      },
      {
        "name": "REMEMBER_ME_SECRET",
        "value": "${remember_me_secret}"
      },
      {
        "name": "SSL_CA_BASE64",
        "value": "${ssl_ca_base64}"
      },
      {
        "name": "IDP_DOMAIN_NAME",
        "value": "${idp_domain_name}"
      },
      {
        "name": "TRUSTED_IP_ADDRESSES",
        "value": "${trusted_ip_addresses}"
      },
      {
        "name": "ANALYTICS_ID",
        "value": "${analytics_id}"
      },
      {
        "name": "HELP_CENTER_URL",
        "value": "${help_center_url}"
      }
    ],
    "links": null,
    "workingDirectory": null,
    "readonlyRootFilesystem": null,
    "image": "${docker_image}",
    "command": ["/data/run-idp.sh"],
    "user": null,
    "dockerLabels": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${cloudwatch_log_group_name}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${app_name}-${app_env}"
      }
    },
    "cpu": ${cpu},
    "privileged": null,
    "memoryReservation": ${memory}
  }
]
